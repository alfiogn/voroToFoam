FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       icoFoam;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           1;

deltaT            1;

writeControl      runTime;

writeInterval     1;

purgeWrite        0;

writeFormat       binary;

writePrecision    10;

writeCompression  uncompressed;

timeFormat        general;

timePrecision     8;

runTimeModifiable true;

adjustTimeStep    no;

maxCo             0.999;

